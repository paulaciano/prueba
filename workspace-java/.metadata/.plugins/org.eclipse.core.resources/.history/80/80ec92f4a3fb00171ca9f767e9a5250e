
public class Ejercicio1 {
	  	int contador1;
	    static int contador2;  

	    public Ejercicio1() {
	         contador1 = 0;
	         contador2 ++;
	    }

	    public void incrementa1() {
	         contador1++;
	    }

	    public void incrementa2() {
	         contador2++;
	    }

	    public static void incrementa() {
	         contador1++;
	         contador2++;
	    }

}
