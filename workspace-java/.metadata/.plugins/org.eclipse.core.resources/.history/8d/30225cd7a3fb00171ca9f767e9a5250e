
public class Examen {
    int contador1;	// Contador 1
    static int contador2; // Contador 2 

    public Lampara() {
         contador1 = 0;
         contador2 ++;
    }

    public void incrementa1() {
         contador1++;
    }

    public void incrementa2() {
         contador2++;
    }

    public static void incrementa() {
         contador1++;
         contador2++;
    }
}
